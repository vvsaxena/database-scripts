ifnull((SELECT MAX(l.created_by) FROM log l WHERE l.table1 = 'customers' AND l.key1 = c.id AND l.action LIKE '%Customer Is Cancelled changed from <em>null</em> to true.%' OR l.action LIKE '%Customer Is Cancelled changed from false to true.%' ),'No Record') AS CanceledById,

