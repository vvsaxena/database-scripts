Optimized query , by just grouping the "OR" condition using paranthesis , query time reduced to 0.02 seconds

SELECT
c.id AS TraxId,
CONCAT(c.fname,' ',c.lname) AS Customer,
DATE_FORMAT(c.sale_date, "%m/%d/%Y") AS CustomerSaleDate,
loc.name AS OfficeLocation,
DATE_FORMAT(c.customer_cancel_date, '%m/%d/%Y') as CancelDate,
mmrs.amount AS MMR,
c.rebate_check AS RebateAmount,
CONCAT(t.months,' ','MONTHS') AS ContractTerm,

CONCAT(IF(c.sale_date IS NOT NULL,IF (t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d"))) < 0,'0',t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d")))), ''),' ','MONTHS') AS MonthsRemaining,

ifnull(DRV.mx_created_by,'No Record') AS CanceledById,

ccr.name AS CancellationReason,

Concat('$', Round(ifnull((SELECT SUM(ili.total_amount)
FROM invoice_line_items ili
WHERE ili.status != 'deleted' AND ili.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')),0), 2)) AS TotalInvoiceAmounts,

Concat('$', Round(ifnull((SELECT SUM(pti.amount_used)
FROM payment_to_invoice pti
WHERE pti.status != 'deleted' AND pti.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')),0), 2)) AS TotalPaymentToInvoiceAmounts,

Concat('$', Round((mmrs.amount*(IF(c.sale_date IS NOT NULL,IF (t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d"))) < 0,'0',t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d")))), '0'))),2)) AS BOC,

Concat('$', Round((SELECT SUM(ili.total_amount)FROM invoice_line_items ili WHERE ili.status != 'deleted' AND ili.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')), 2) - Round(ifnull((SELECT SUM(pti.amount_used)FROM payment_to_invoice pti WHERE pti.status != 'deleted' AND pti.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')),0), 2)) AS TotalInvoiceAmountDue,


Concat('$', Round((mmrs.amount*(IF(c.sale_date IS NOT NULL,IF (t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d"))) < 0,'0',t.months-(TIMESTAMPDIFF(MONTH, c.sale_date, DATE_FORMAT(NOW(), "%Y-%m-%d")))), '0'))),2) +



Round(ifnull((SELECT SUM(ili.total_amount)FROM invoice_line_items ili WHERE ili.status != 'deleted' AND ili.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')),0), 2)
- Round(ifnull((SELECT SUM(pti.amount_used)FROM payment_to_invoice pti WHERE pti.status != 'deleted' AND pti.invoice_id IN(SELECT i.id FROM invoices i WHERE i.entity_type = 'customer' AND i.entity_id = c.id AND i.status != 'recurring' AND i.status != 'deleted')),0), 2)) AS TotalDue


FROM customers c

LEFT JOIN
(
SELECT key1 as CID,MAX(created_by) as mx_created_by FROM log
WHERE table1 = 'customers'
AND (action LIKE '%Customer Is Cancelled changed from <em>null</em> to true.%'
OR action LIKE '%Customer Is Cancelled changed from false to true.%')
group by key1
) as DRV on DRV.CID = c.id

LEFT JOIN customer_cancel_reasons ccr ON ccr.id = c.customer_cancel_reason_id
LEFT JOIN mmrs ON c.mmr_id = mmrs.id
LEFT JOIN locations loc ON c.location_id = loc.id
LEFT JOIN terms t ON t.id = c.term_id
LEFT JOIN invoices i ON i.entity_id = c.id AND i.entity_type = 'customer' AND i.status != 'recurring'
LEFT JOIN invoice_line_items ili ON ili.invoice_id = i.id
  WHERE c.status = 'active'   AND DATE_FORMAT(c.customer_cancel_date, '%Y-%m-%d') >= '2019-06-01'   AND DATE_FORMAT(c.customer_cancel_date, '%Y-%m-%d') <= '2019-06-18'    GROUP BY c.id  ORDER BY c.id DESC
;
